    {
        "manifest_version": 2,
        "name": "JutsuSkipMan",
        "version": "1.0",
        "description": "Skips all buttons , such as: skip intro/outro, next season/episode",
        "icons": {
            "48": "icons/JutsuSkipManLogo.png"
        },
        "content_scripts": [
            {
                "matches": [
                    "*://jut.su/*"
                ],
                "js": [
                    "skip.js"
                ]
            }
        ]
    }